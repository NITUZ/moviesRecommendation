      <Label fx:id="movieTitle_txt" layoutX="121.0" layoutY="255.0" prefHeight="17.0" prefWidth="333.0" />
      <Label fx:id="movieRank_txt" layoutX="147.0" layoutY="285.0" prefHeight="17.0" prefWidth="121.0" />
      <Button fx:id="prev_btn" layoutX="69.0" layoutY="315.0" mnemonicParsing="false" onAction="#prevMovie" text="prev" />
      <Button fx:id="next_btn" layoutX="428.0" layoutY="315.0" mnemonicParsing="false" onAction="#nextMovie" text="next" />
   </children>
</AnchorPane>
