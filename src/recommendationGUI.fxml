<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RecommendationGUIController">
   <children>
      <Label layoutX="154.0" layoutY="32.0" text="Recommended for you!">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
      <ImageView fx:id="moviePhoto" fitHeight="150.0" fitWidth="200.0" layoutX="190.0" layoutY="151.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="prev_btn" disable="true" layoutX="78.0" layoutY="320.0" mnemonicParsing="false" onAction="#prevMovie" text="previous" />
      <Button fx:id="next_btn" layoutX="400.0" layoutY="320.0" mnemonicParsing="false" onAction="#nextMovie" prefHeight="25.0" prefWidth="62.0" text="next" />
      <Label layoutX="194.0" layoutY="72.0" prefHeight="17.0" prefWidth="34.0" text="Title:" />
      <Label fx:id="movieTitle_txt" layoutX="231.0" layoutY="72.0" prefHeight="17.0" prefWidth="175.0" />
      <Label layoutX="197.0" layoutY="101.0" prefHeight="17.0" prefWidth="34.0" text="Rank:" />
      <Label fx:id="movieRank_txt" layoutX="231.0" layoutY="101.0" prefHeight="17.0" prefWidth="43.0" />
   </children>
</AnchorPane>
